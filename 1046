#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,a1,a2,b1,b2,sum=0,a=0,b=0,i;
    scanf("%d",&n);
    for(i=0;i<n;i++){
    sum=0;
    scanf("%d %d %d %d",&a1,&a2,&b1,&b2);
    sum=a1+b1;
    if(a2==sum && a2!=b2){
       b++;
    }
     if(b2==sum && a2!=b2){
       a++;
    }

    }
    printf("%d %d",a,b);
    return 0;
}
